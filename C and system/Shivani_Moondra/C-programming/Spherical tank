#include <stdio.h>
float r,v,h;
int main()
{
     scanf("%f",&r);  // enter the radius
     scanf("%f",&v); // enter the volume
     h = (v/(3.14*r*r));  // volume of spherical tank is v=3.14*r*r*h  
     printf("depth of the tank is = %f",h);

    return 0;
}
