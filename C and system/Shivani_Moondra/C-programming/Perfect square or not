/******************************************************************************
Number is perfect square or not.

*******************************************************************************/
#include <stdio.h>
#include <math.h>
int n=25;
int main()
{
    int a=sqrt(n);
    if(n==(a*a))
    {
        printf("yes");
    }
    else
    {
        printf("no");
    }
    return 0;
}
